<% unless @review.nil? %>
  $(".modal-window").html("<%=j render partial: 'users/reviews/modal', locals: { review: @review } %>");  
  $(".modal-state").prop('checked', true);
<% end %>

$(document).ready(function() {
  $(".review-star:lt(<%= @review.rating %>)").removeClass("fa-star-o").addClass("fa-star");
  give_reviews();
});